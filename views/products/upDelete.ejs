<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="stylesheets/style.css">
    <link href="https://fonts.googleapis.com/css2?family=Cinzel+Decorative:wght@700&display=swap" rel="stylesheet">

    <title>Document</title>
</head>
<body>

    <table>
        <thead>
          <tr>
            <th>Product</th>
            <th>Description</th>
            <th>Price</th>
            <th>Update Info</th>
            <th>Delete Info</th>
          </tr>
        </thead>
        <tbody>
          <% productDatabase.forEach(function(p) { %>
            <tr>
              <td><%= p?.title %></td>
              <td><%= p?.description %></td>
              <td><%= p?.price %></td>
              <td><a href="/products/<%= p?._id %>/update">UPDATE</a></td>
              <td><a href="/products/<%= p?._id %>/delete">DELETE</a></td>
            </tr>
            <% }); %>
          </tbody>
        </table>
    
</body>
</html>